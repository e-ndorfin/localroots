{
  "name": "scaffold-xrp",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^1.13.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.0",
  "pnpm": {
    "overrides": {
      "next@>=13.4.0 <14.1.1": ">=14.1.1",
      "next@>=14.0.0 <14.2.10": ">=14.2.10",
      "next@>=10.0.0 <14.2.7": ">=14.2.7",
      "next@>=14.0.0 <14.2.21": ">=14.2.21",
      "next@>=13.0 <14.2.30": ">=14.2.30",
      "next@>=0.9.9 <14.2.31": ">=14.2.31",
      "next@>=9.5.5 <14.2.15": ">=14.2.15",
      "next@>=0.9.9 <14.2.32": ">=14.2.32",
      "next@>=0.9.9 <14.2.24": ">=14.2.24",
      "next@>=14.0.0 <14.2.25": ">=14.2.25"
    }
  }
}
